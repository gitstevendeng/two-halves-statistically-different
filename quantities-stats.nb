Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Wstat", "[", 
    RowBox[{"v1_List", ",", "v2_List"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"rankedsigns", ",", "rankedabs", ",", "temp", ",", "Ri"}], 
      "}"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"rankedsigns", ",", "rankedabs"}], "}"}], "=", 
       RowBox[{"DeleteCases", "[", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"delete", " ", "|", 
           RowBox[{"x1", "-", "x2"}], "|"}], "=", 
          RowBox[{
           RowBox[{"0", " ", "and", " ", "sign", 
            RowBox[{"(", ")"}]}], "=", "0"}]}], "*)"}], 
        RowBox[{
         RowBox[{"Transpose", "[", 
          RowBox[{"SortBy", "[", 
           RowBox[{
            RowBox[{"MapThread", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"Sign", "[", 
                  RowBox[{"#1", "-", "#2"}], "]"}], ",", 
                 RowBox[{"Abs", "[", 
                  RowBox[{"#1", "-", "#2"}], "]"}]}], "}"}], "&"}], ",", 
              RowBox[{"{", 
               RowBox[{"v1", ",", "v2"}], "}"}]}], "]"}], ",", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "&"}]}], "]"}], "]"}], ",", 
         RowBox[{"0", "|", "0."}], ",", 
         RowBox[{"{", "2", "}"}]}], "]"}]}], 
      RowBox[{"(*", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"ranked", " ", "signs"}], ",", " ", 
           RowBox[{
           "differ", " ", "by", " ", "a", " ", "sign", " ", "from", " ", 
            "the", " ", "paper"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"ranked", "|", 
           RowBox[{
            SubscriptBox["x", 
             RowBox[{"1", ",", "i"}]], "-", 
            SubscriptBox["x", 
             RowBox[{"2", ",", "i"}]]}], "|"}], "}"}]}], "}"}], "*)"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"temp", "=", 
       RowBox[{"GatherBy", "[", 
        RowBox[{
         RowBox[{"MapIndexed", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"#2", "[", 
               RowBox[{"[", "1", "]"}], "]"}], ",", "#1"}], "}"}], "&"}], ",",
            "rankedabs"}], "]"}], ",", 
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"temp", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Total", "[", 
             RowBox[{"#", "[", 
              RowBox[{"[", 
               RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}], "/", 
            RowBox[{"Length", "[", "#", "]"}]}], ",", 
           RowBox[{"Length", "[", "#", "]"}]}], "}"}], "&"}], "/@", 
        "temp"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Ri", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"ConstantArray", "[", 
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"#", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "&"}], "/@", "temp"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Total", "[", 
       RowBox[{"rankedsigns", "*", "Ri"}], "]"}]}]}], 
    RowBox[{"(*", "W", "*)"}], "]"}]}], ";"}]], "Input",
