      RowBox[{"tokens1", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Import", "[", 
         RowBox[{"file", "<>", "\"\<tokens_1.dat\>\""}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"tokens2", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Import", "[", 
         RowBox[{"file", "<>", "\"\<tokens_2.dat\>\""}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"fws1", "=", 
       RowBox[{"ReverseSort", "[", 
        RowBox[{"Counts", "[", "tokens1", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"fws2", "=", 
       RowBox[{"ReverseSort", "[", 
        RowBox[{"Counts", "[", "tokens2", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"selectedkeys", "=", 
       RowBox[{"Union", "[", 
        RowBox[{"Flatten", "[", 
         RowBox[{"Keys", "[", 
          RowBox[{"KeyIntersection", "[", 
           RowBox[{"{", 
            RowBox[{"fws1", ",", "fws2"}], "}"}], "]"}], "]"}], "]"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"selected1", "=", 
       RowBox[{"KeySelect", "[", 
        RowBox[{"fws1", ",", 
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"selectedkeys", ",", "#"}], "]"}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"selected2", "=", 
       RowBox[{"KeySelect", "[", 
        RowBox[{"fws2", ",", 
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{"selectedkeys", ",", "#"}], "]"}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"C1", "=", 
       RowBox[{"Length", "[", "selected1", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"C2", "=", 
       RowBox[{"Length", "[", "selected2", "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"C", ":", " ", 
        RowBox[{
         RowBox[{"number", " ", "of", " ", "different", " ", "common", " ", 
          RowBox[{"words", ".", " ", "C1"}]}], "\[Congruent]", "C", 
         "\[Congruent]", 
         RowBox[{"C2", " ", "should", " ", "hold"}]}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"CC1", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"Values", "[", "selected1", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"CC2", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"Values", "[", "selected2", "]"}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"CC", ":", " ", 
        RowBox[{
        "total", " ", "number", " ", "of", " ", "common", " ", "words", " ", 
         "between", " ", "the", " ", "two", " ", "halves"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"c1", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"Total", "[", 
          RowBox[{"Values", "[", "selected1", "]"}], "]"}], "/", 
         RowBox[{"Total", "[", 
          RowBox[{"Values", "[", "fws1", "]"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c2", "=", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"Total", "[", 
          RowBox[{"Values", "[", "selected2", "]"}], "]"}], "/", 
         RowBox[{"Total", "[", 
          RowBox[{"Values", "[", "fws2", "]"}], "]"}]}], "]"}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"c", ":", " ", 
        RowBox[{
        "overall", " ", "frequency", " ", "of", " ", "common", " ", "words", 
         " ", "in", " ", "each", " ", "half"}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
       "file", ",", "C1", ",", "C2", ",", "CC1", ",", "CC2", ",", "c1", ",", 
        "c2"}], "}"}]}]}], "]"}]}], ";"}]], "Input",
